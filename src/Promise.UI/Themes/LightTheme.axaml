<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<Color x:Key="MainThemeColor">#ffffff</Color>
	
	<!-- Colors for font -->
	<Color x:Key="FontStrongThemeColor">#000000</Color>
	<Color x:Key="FontMediumThemeColor">#050505</Color>
	<Color x:Key="FontLightThemeColor"> #101010</Color>

	<!-- Uniform colors -->
	<Color x:Key="HighUniformThemeColor">  #ebebeb</Color>
	<Color x:Key="MediumUniformThemeColor">#d6d6d6</Color>
	<Color x:Key="LowUniformThemeColor">   #c9c9c9</Color>
	
	<!-- Contrast colors -->
	<Color x:Key="HighContrastThemeColor">  #101010</Color>
	<Color x:Key="MediumContrastThemeColor">#202020</Color>
	<Color x:Key="LowContrastThemeColor">   #303030</Color>
</ResourceDictionary>

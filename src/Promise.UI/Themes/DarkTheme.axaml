<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<SolidColorBrush x:Key="SolidThemeColor" Color="#000000"/>

	<!-- Colors for font -->
	<SolidColorBrush x:Key="FontStrongThemeColor" Color="#ffffff"/>
	<SolidColorBrush x:Key="FontMediumThemeColor" Color="#f0f0f0"/>
	<SolidColorBrush x:Key="FontLightThemeColor" Color="#e0e0e0"/>

	<!-- Uniform colors -->
	<SolidColorBrush x:Key="HighUniformThemeColor" Color="#141414"/>
	<SolidColorBrush x:Key="MediumUniformThemeColor" Color="#181818"/>
	<SolidColorBrush x:Key="LowUniformThemeColor" Color="#252525"/>

	<!-- Contrast colors -->
	<SolidColorBrush x:Key="HighContrastThemeColor" Color="#f0f0f0"/>
	<SolidColorBrush x:Key="MediumContrastThemeColor" Color="#e0e0e0"/>
	<SolidColorBrush x:Key="LowContrastThemeColor" Color="#d0d0d0"/>
</ResourceDictionary>

<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<Color x:Key="MainThemeColor">#000000</Color>

	<!-- Colors for font -->
	<Color x:Key="FontStrongThemeColor">#ffffff</Color>
	<Color x:Key="FontMediumThemeColor">#f0f0f0</Color>
	<Color x:Key="FontLightThemeColor"> #e0e0e0</Color>

	<!-- Uniform colors -->
	<Color x:Key="HighUniformThemeColor">  #141414</Color>
	<Color x:Key="MediumUniformThemeColor">#181818</Color>
	<Color x:Key="LowUniformThemeColor">   #252525</Color>

	<!-- Contrast colors -->
	<Color x:Key="HighContrastThemeColor">  #f0f0f0</Color>
	<Color x:Key="MediumContrastThemeColor">#e0e0e0</Color>
	<Color x:Key="LowContrastThemeColor">   #d0d0d0</Color>
</ResourceDictionary>

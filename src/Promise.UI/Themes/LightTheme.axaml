<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<SolidColorBrush x:Key="SolidThemeColor" Color="#ffffff"/>

	<!-- Colors for font -->
	<SolidColorBrush x:Key="FontStrongThemeColor" Color="#000000"/>
	<SolidColorBrush x:Key="FontMediumThemeColor" Color="#050505"/>
	<SolidColorBrush x:Key="FontLightThemeColor" Color="#101010"/>

	<!-- Uniform colors -->
	<SolidColorBrush x:Key="HighUniformThemeColor" Color="#ebebeb"/>
	<SolidColorBrush x:Key="MediumUniformThemeColor" Color="#d6d6d6"/>
	<SolidColorBrush x:Key="LowUniformThemeColor" Color="#c9c9c9"/>
	
	<!-- Contrast colors -->
	<SolidColorBrush x:Key="HighContrastThemeColor" Color="#101010"/>
	<SolidColorBrush x:Key="MediumContrastThemeColor" Color="#202020"/>
	<SolidColorBrush x:Key="LowContrastThemeColor" Color="#303030"/>
</ResourceDictionary>
